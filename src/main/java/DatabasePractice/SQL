drop table workers;
drop table registration;

CREATE TABLE WORKERS(
employeeid int auto_increment primary key,
name varchar(100),
department varchar(100),
location varchar(10), 
salary int)
		

INSERT INTO WORKERS VALUES (1,'Zara','Sales','Dhaka',10000)

INSERT INTO WORKERS VALUES (2,'Abdul', 'Sales','Gazipur',20000)
			
INSERT INTO WORKERS VALUES (3,'Harun', 'Store','Gazipur',30000)
			
INSERT INTO WORKERS VALUES (4,'Zia', 'Store','Dhaka',40000)

SELECT MAX(SALARY) FROM WORKERS